<section class="Layout-Content row center-lg">
  <section class="Semana col-lg-10">

    <div class="Dia">
      <div class="row start-lg">
        <div class="Dia-Title col-lg-12">
          <p>Lunes</p>
          <div class="divider"></div>
        </div>
        <div class="Dia-Content col-lg-12">
            <div class="Turnos">
              <div id="Turnos-Lunes" class="row center-lg">

              </div>
            </div>
        </div>
      </div>
    </div>

  </section>
</section>


<section class="Turno col-lg-5 card">
  <div class="Turno-Content">
    <div class="row start-lg">
      <div class="Turno-Info col-lg-4">
        <h5>{{ turno.name }}</h5>
        <p><{{ turno.start + ' - '+turno.end }}</p>
      </div>
      <div class="Turno-Log col-lg-4">
        <h4>{{ turno.cupo}} </h4>
      </div>
      <div class="Turno-Button col-lg-4">
        <form class="Send-Button" accept-charset="utf-8">
          <input type="hidden" name="_method" value="Clear"/>
          <button ng-click="sendTurno()" class="btn-flat material-icons" value="close" type="submit"></button>
          <input type="hidden" name="_csrf" value="<%= _csrf %>"/>
        </form>
      </div>
    </div>
  </div>
</section>
