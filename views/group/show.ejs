    <section class="Main">
      <section class="Group">
        <div class="Filter row center-xs">
          <button class="btn">Todos</button>
          <input type="text" placeholder="Buscar...">
        </div>
        <div class="Group-Items row center-xs">
          <table class="col-xs-11 bordered">
            <thead>
              <tr>
                <th>Nombre</th>
                <th>Email</th>
                <th>Telefono</th>
                <th>Direccion</th>
                <th>Grupos</th>
                <th>Tokens</th>
              </tr>
            </thead>
            <tbody>

              <% _.each(users, function(user){ %> 
                
                <tr>
                  <td><%= user.name + ' ' + user.lastname %></td>
                  <td><%= user.email %></td>
                  <td><%= user.phone %></td>
                  <td><%= user.adress %></td>
                  <td>
                    <div class="input-field">
                     <select multiple>
                       <option value="" disabled selected>Choose your option</option>
                       <option value="1">Option 1</option>
                       <option value="2">Option 2</option>
                       <option value="3">Option 3</option>
                     </select>
                     <label>Materialize Multiple Select</label>
                   </div>
                  </td>
                  <td>
                    <input>
                  </td>
                  <td> <button class="btn btn-floating bc-green"><a href="/user/edit/<%= user.id %>" class="material-icons">Edit</a></button> </td>
                  <td>
                    <form action="/user/destroy/<%= user.id %>" method="POST" accept-charset="utf-8">
                      <input type="hidden" name="_method" value="Clear"/>
                      <input type="submit" class="btn btn-floating bc-green" value="Clear"></input>
                      <input type="hidden" name="_csrf" value="<%= _csrf %>"/>
                    </form>
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </section>
    </section>